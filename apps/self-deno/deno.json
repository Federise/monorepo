{
  "name": "@federise/self-deno",
  "version": "0.1.0",
  "exports": "./src/main.ts",
  "unstable": ["kv", "sloppy-imports"],
  "tasks": {
    "dev": "deno run --allow-net --allow-env --allow-read --allow-write --allow-sys --env --watch src/main.ts",
    "start": "deno run --allow-net --allow-env --allow-read --allow-write --allow-sys --env src/main.ts",
    "test": "deno test --allow-net --allow-env test/",
    "test:watch": "deno test --allow-net --allow-env --watch test/",
    "compile": "deno compile --allow-net --allow-env --allow-read --allow-write --allow-sys --output federise-gateway src/main.ts",
    "compile:linux": "deno compile --allow-net --allow-env --allow-read --allow-write --target x86_64-unknown-linux-gnu --output federise-gateway-linux src/main.ts",
    "compile:macos": "deno compile --allow-net --allow-env --allow-read --allow-write --target x86_64-apple-darwin --output federise-gateway-macos src/main.ts",
    "compile:macos-arm": "deno compile --allow-net --allow-env --allow-read --allow-write --target aarch64-apple-darwin --output federise-gateway-macos-arm src/main.ts",
    "compile:windows": "deno compile --allow-net --allow-env --allow-read --allow-write --target x86_64-pc-windows-msvc --output federise-gateway.exe src/main.ts"
  },
  "imports": {
    "@federise/gateway-core": "../../packages/gateway-core/src/index.ts",
    "@federise/gateway-core/": "../../packages/gateway-core/src/",
    "hono": "npm:hono@^4.6.20",
    "hono/cors": "npm:hono@^4.6.20/cors",
    "chanfana": "npm:chanfana@^2.6.3",
    "zod": "npm:zod@^3.24.1",
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@^3.958.0",
    "@aws-sdk/s3-request-presigner": "npm:@aws-sdk/s3-request-presigner@^3.958.0"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitOverride": false,
    "lib": ["deno.window", "deno.unstable"]
  }
}
