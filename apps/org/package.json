{
  "name": "federise-org",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "generate:schema": "npx openapi-typescript http://localhost:3000/openapi -o src/api/schema.ts",
    "dev": "pnpm build:sw && astro dev",
    "dev:sw": "cd service-worker && pnpm watch",
    "build": "pnpm build:sw && astro build && pnpm build:sw:full && pnpm build:sw:copy",
    "build:sw": "cd service-worker && pnpm build",
    "build:sw:full": "cd service-worker && pnpm generate && pnpm build",
    "build:sw:copy": "cp public/sw.js dist/sw.js && cp public/sw.js.map dist/sw.js.map",
    "preview": "pnpm build && wrangler dev",
    "astro": "astro",
    "check": "astro check && svelte-check --tsconfig ./tsconfig.json",
    "check:astro": "astro check",
    "check:svelte": "svelte-check --tsconfig ./tsconfig.json",
    "deploy": "pnpm build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "test:frame": "cd tests && pnpm dev",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check ."
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/svelte": "^7.2.4",
    "@federise/proxy": "workspace:*",
    "@federise/sdk": "workspace:*",
    "astro": "^5.16.6",
    "openapi-fetch": "^0.13.3",
    "svelte": "^5.2.9",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.3.2",
    "svelte-check": "^4.3.5",
    "wrangler": "^4.54.0"
  }
}
