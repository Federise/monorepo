{
  "name": "federise-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel --stream dev",
    "build": "pnpm --stream -r build",
    "test": "pnpm --stream -r test",
    "test:e2e": "cd test/e2e && pnpm test",
    "test:e2e:headed": "cd test/e2e && pnpm test:headed",
    "clean": "pnpm --stream -r clean && rm -rf node_modules",
    "generate:api": "cd apps/gateway && pnpm generate-openapi && cd ../org && pnpm generate:api"
  },
  "packageManager": "pnpm@10.26.0",
  "devDependencies": {
    "playwright": "^1.57.0"
  }
}
