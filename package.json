{
  "name": "federise-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel --stream dev",
    "build": "pnpm --stream -r build",
    "test": "pnpm --stream -r test",
    "clean": "pnpm --stream -r clean && rm -rf node_modules",
    "generate:api": "pnpm --filter federise-gateway generate-openapi && pnpm --filter federise-org generate:api"
  },
  "packageManager": "pnpm@10.26.0"
}
