{
  "name": "@federise/manage-ui",
  "version": "0.1.0",
  "description": "Federise management UI - reusable Svelte components and utilities for gateway management",
  "type": "module",
  "svelte": "./src/index.ts",
  "exports": {
    ".": {
      "svelte": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./components": {
      "svelte": "./src/components/index.ts",
      "types": "./src/components/index.ts"
    },
    "./lib": {
      "svelte": "./src/lib/index.ts",
      "import": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./utils": {
      "svelte": "./src/utils/index.ts",
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "openapi-fetch": "^0.13.3"
  },
  "peerDependencies": {
    "@federise/proxy": "workspace:*",
    "@federise/sdk": "workspace:*",
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@federise/proxy": "workspace:*",
    "@federise/sdk": "workspace:*",
    "svelte": "^5.2.9",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
