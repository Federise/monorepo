{
  "name": "@federise/proxy",
  "version": "0.1.0",
  "description": "Federise proxy package - reusable message routing and backend abstraction",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./backends": {
      "import": "./dist/backends/index.js",
      "types": "./dist/backends/index.d.ts"
    },
    "./transports": {
      "import": "./dist/transports/index.js",
      "types": "./dist/transports/index.d.ts"
    },
    "./capabilities": {
      "import": "./dist/capabilities/index.js",
      "types": "./dist/capabilities/index.d.ts"
    },
    "./vault": {
      "import": "./dist/vault/index.js",
      "types": "./dist/vault/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "peerDependencies": {
    "@federise/sdk": "workspace:*"
  },
  "devDependencies": {
    "@federise/sdk": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
